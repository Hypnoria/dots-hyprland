        {
            "type": "lm",
            "key": "│ ├",
            "keyColor": "red"
        },
        {
            "type": "wm",
            "key": "│ ├",
            "keyColor": "red"
        },
        {
            "type": "wmtheme",
            "key": "│ ├󰉼",
            "keyColor": "red"
